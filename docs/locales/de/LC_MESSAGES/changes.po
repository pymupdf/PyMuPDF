msgstr ""

#: ../../../changes.txt:1529 7e33262c6b504f278e6ec5f30135a24b
msgid "**Changes in Version 1.18.13**"
msgstr ""

#: ../../../changes.txt:1531 e331d7b0d02f455d897a482a2f066edf
